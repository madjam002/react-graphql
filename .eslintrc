---
  parser: "babel-eslint"

  extends: "standard"

  rules:
    space-before-function-paren: [2, {"anonymous": "always", "named": "never"}]
    comma-dangle: [2, "always-multiline"]
    no-var: 2
    prefer-const: 2
    padded-blocks: 0
    arrow-parens: 0
    no-multiple-empty-lines: 0
    no-return-assign: 0
    generator-star-spacing: 0
    no-duplicate-imports: 0
