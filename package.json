{
  "name": "react-graphql",
  "version": "0.0.3",
  "description": "A simple but powerful GraphQL client for React",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --copy-files",
    "watch": "rm -rf lib && babel src --out-dir lib -w --copy-files",
    "lint": "eslint .",
    "test": "npm run lint",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madjam002/react-graphql.git"
  },
  "keywords": [
    "graphql",
    "simple",
    "cache",
    "client",
    "react"
  ],
  "author": "Jamie Greeff <jamie@greeff.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madjam002/react-graphql/issues"
  },
  "homepage": "https://github.com/madjam002/react-graphql#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "graphql-tag": "^0.1.14",
    "react": "^15.3.2"
  },
  "dependencies": {
    "graphql-cache": "^0.3.0",
    "graphql-tag": "^0.1.14",
    "recompose": "^0.20.2",
    "uuid": "^3.0.1"
  }
}
